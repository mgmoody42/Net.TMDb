<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Hasseware.TheMovieDB</id>
    <version>1.2.5.0</version>
    <title>The Movie Database .NET Client</title>
    <authors>Miguel Hasse de Oliveira</authors>
    <owners>Miguel Hasse de Oliveira</owners>
    <licenseUrl>https://github.com/miguelhasse/Net.TMDb/raw/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/miguelhasse/Net.TMDb</projectUrl>
    <iconUrl>https://github.com/miguelhasse/Net.TMDb/raw/master/.nuget/Package.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Client library for accessing the TMDb API in .NET, that is lightweight and simple to use.

      Supported Platforms:
      - .NET Framework 4.5
      - Windows 8
      - Windows Phone 8.1
      - Windows Phone Silverlight 8
      - Portable Class Libraries
    </description>
    <summary>Client library for accessing the TMDb API in .NET</summary>
    <releaseNotes>
      Episode.ProductionCode type changed from int to string.
      Included new TV show methods (accessible in interface IShowInfo).
    </releaseNotes>
    <copyright>Copyright 2014 Miguel Hasse de Oliveira</copyright>
    <language>en-US</language>
    <tags>TMDb PCL Portable Movie</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Newtonsoft.Json" version="6.0.6" />
      </group>
      <group targetFramework=".NETPortable0.0-net45+win8+wp8+wpa81">
        <dependency id="Microsoft.Net.Http" version="2.2.28" />
        <dependency id="Newtonsoft.Json" version="6.0.6" />
      </group>
    </dependencies>
  </metadata>
